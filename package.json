{
  "name": "vite-plugin-deploy-oss",
  "version": "3.0.0",
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "type": "module",
  "homepage": "https://github.com/yulin96/vite-plugin-deploy-oss",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/yulin96/vite-plugin-deploy-oss.git"
  },
  "bugs": {
    "url": "https://github.com/yulin96/vite-plugin-deploy-oss/issues"
  },
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "default": "./dist/index.js"
    }
  },
  "scripts": {
    "build": "tsup",
    "typecheck": "tsc -p tsconfig.json",
    "pack": "pnpm run build && pnpm pack",
    "build:test": "cd playground && vite build",
    "build:test:deploy": "cd playground && vite build --mode deploy"
  },
  "keywords": [
    "vite-plugin",
    "vite",
    "plugin",
    "oss",
    "alioss",
    "deploy",
    "upload"
  ],
  "author": "yulinzhao",
  "license": "MIT",
  "description": "将dist目录下的文件上传到阿里云oss",
  "devDependencies": {
    "@types/ali-oss": "^6.23.3",
    "@types/node": "^22.19.13",
    "tsup": "^8.5.1",
    "typescript": "^5.9.3"
  },
  "peerDependencies": {
    "vite": "^6.0.3 || ^7"
  },
  "dependencies": {
    "ali-oss": "^6.23.0",
    "chalk": "^5.6.2",
    "glob": "^13.0.6",
    "ora": "^9.3.0"
  }
}
